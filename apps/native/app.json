{
  "expo": {
    "scheme": "styx",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "bundler": "metro"
    },
    "name": "styx",
    "slug": "styx",
    "plugins": [
      "expo-font",
      [
        "@kingstinct/react-native-healthkit",
        {
          "NSHealthShareUsageDescription": "Styx reads your Health data to sync your personal dashboard.",
          "NSHealthUpdateUsageDescription": "Styx writes approved calorie entries to Health for two-way sync.",
          "background": true
        }
      ]
    ],
    "ios": {
      "infoPlist": {
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.expo.modules.backgroundtask.processing"
        ],
        "UIBackgroundModes": [
          "processing"
        ]
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
